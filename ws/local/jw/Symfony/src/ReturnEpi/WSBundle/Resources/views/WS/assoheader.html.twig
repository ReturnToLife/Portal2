{% if app.session.get("student") != null %}
	<div style="float:right;"><a href="{{ path('WS_accueil') }}">Index Return</a></div>
	<br /><form method="post" name="dcform">
		<strong>Consulter :</strong><br /><br />
		{% for item in visible %}
			{% if loop.index % 4 == 0 %}
				<br />
			{% endif %}
			 &nbsp;&nbsp; <a href="{{ path('WS_asso', {'name': name, 'tool' : item['name']}) }}">
			{{ item["name"] }}</a>
		{% endfor %}
		<input type="submit" value="Disconnect" name="senddc" id="senddc" style="float:right;" />
	</form><br />
{% else %}
<br /><form method="post" name="loginform"><strong>
	Login <input type="text" name="login" id="login" size="10" />&nbsp;&nbsp;
	Password <input type="password" name="mdp" id="mdp" size="10" />
	<input type="submit" value="Login" name="sendlogin" id="sendlogin" 
	style="float:right; display:block;" />
</strong></form><br />
{% endif %}

